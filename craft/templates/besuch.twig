{% extends "_layout" %}
{% set title = "Besuch" %}

{% block content %}

<div class="visit text-block">		
	<section>
			<div>
				<header class="section-header">
					<h1>Ihr Besuch</h1>
				</header>
				{{ entry.text }}
			</div>
	</section>
	<section>
			<div class="narrow">
				
				<div>
					{{ entry.tickets }}
				</div>
				<div class="presell">
					<div>
						{{ entry.presell }}
					</div>
					<presell></presell>
				</div>
				<div>
					{{ entry.boxOffice }}
				</div>
			</div>

	</section>
	{% for block in entry.additionalInfos %}
		<section>
				<div>
					{{ block.text }}
				</div>
		</section>
	{% endfor %}
	
</div>
{% endblock %}