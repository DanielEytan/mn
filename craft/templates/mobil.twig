{% extends "_layout" %}
{% set title = "Besuch" %}

{% block content %}

<div class="visit text-block">		
	<section>
			<div class="narrow">
				<header class="section-header">
					<h1>Mobil durch die Nacht</h1>
				</header>
				{{ entry.text }}
			</div>
	</section>
	{% for block in entry.textBlock %}
		<section>
				<div class="narrow">
					{{ block.text }}
				</div>
		</section>
	{% endfor %}
</div>
{% endblock %}