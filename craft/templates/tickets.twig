{% extends "_layout" %}
{% set title = "Tickets" %}
{% set mt = "mt" %}

{% block content %}
<header class="site-header">
		<h1>Tickets</h1>
	</header>
<section class="tickets">
	
	<div class="info-block">		

			<div>
				{{ entry.tickets }}
			</div>
			<div class="presell">
				<div>
					{{ entry.presell }}
				</div>
				{# <presell></presell> #}
				{% for block in entry.boxOfficeLocations %}
					<div>
						{{ block.list}}
					</div>
					
				{% endfor %}
			</div>
			<div>
				{{ entry.boxOffice }}
			</div>


	</div>
</section>

{% endblock %}